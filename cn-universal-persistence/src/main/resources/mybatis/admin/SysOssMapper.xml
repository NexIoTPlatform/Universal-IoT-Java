<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.universal.ossm.mapper.SysOssMapper">

  <resultMap type="cn.universal.ossm.entity.SysOss" id="SysOssResult">
    <result property="ossId" column="oss_id"/>
    <result property="fileName" column="file_name"/>
    <result property="fileSuffix" column="file_suffix"/>
    <result property="url" column="url"/>
    <result property="createTime" column="create_time"/>
    <result property="createBy" column="create_by"/>
    <result property="updateTime" column="update_time"/>
    <result property="updateBy" column="update_by"/>
    <result property="service" column="service"/>
  </resultMap>

  <select id="listByIds" parameterType="collection" resultMap="SysOssResult">
    select * from sys_oss where oss_id in
    <foreach item="id" collection="array" open="(" separator="," close=")">
      #{id}
    </foreach>
  </select>
  <select id="listByUrls" parameterType="String" resultMap="SysOssResult">
    select * from sys_oss where url in
    <foreach item="id" collection="array" open="(" separator="," close=")">
      #{id}
    </foreach>
  </select>
  <delete id="removeByIds" parameterType="collection">
    delete from sys_oss where oss_id in
    <foreach item="id" collection="array" open="(" separator="," close=")">
      #{id}
    </foreach>
  </delete>
  <delete id="removeByUrls" parameterType="String">
    delete from sys_oss where url in
    <foreach item="id" collection="array" open="(" separator="," close=")">
      #{id}
    </foreach>
  </delete>
</mapper>
